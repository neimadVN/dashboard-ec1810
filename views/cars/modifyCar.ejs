<div class="alert alert-danger" id="notificationAlert" style="display: none;">
</div>

<div class="form-group">
    <label for="id">Mã xe:</label>
    <input type="text" class="form-control" id="objectId" disabled="true" value="<%= car.objectId %>" />
</div>

<div class="form-group">
    <label for="loaiXe">Loại xe:</label>
    <select id="loaiXe" name="loaiXe" autocomplete="off" class="form-control mw-100" required group-input-data-parsley>
        <% loaiXe.forEach(loai => { %>
        <% if (loai.objectId === carType) { %>
        <option selected value="<%= loai.objectId %>">
            <%= loai.tenLoai %>
        </option>
        <% } else { %>
        <option value="<%= loai.objectId %>">
            <%= loai.tenLoai %>
        </option>
        <% }}); %>
    </select>
</div>

<div class="form-group">
    <label for="bienSo">Biển số:</label>
    <input type="text" class="form-control" id="bienSo" value="<%= car.bienSo %>" />
</div>

<div class="form-group">
    <label for="chiTiet">Mô tả:</label>
    <input type="text" class="form-control" id="chiTiet" value="<%= car.chiTiet %>" />
</div>

<div class="form-group">
        <label for="trangThai">Trạng thái:</label>
        <select id="trangThai" name="trangThai" autocomplete="off" class="form-control mw-100" required
            group-input-data-parsley>
            <% if (car.trangThai === 'ACTIVED') { %>
            <option selected value="ACTIVED" class="text-active">
                Còn hoạt động
            </option>
            <option value="INACTIVED" class="text-inactive">
                Ngưng hoạt động
            </option>
            <% } else { %>
            <option value="ACTIVED" class="text-active">
                Còn hoạt động
            </option>
            <option selected value="INACTIVED" class="text-inactive">
                Ngưng hoạt động
            </option>
            <% }; %>
        </select>
    </div>